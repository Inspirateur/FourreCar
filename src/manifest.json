{
  "manifest_version": 2,
  "name": "FourreCar",
  "version": "0.1.0",
  "description": "Facilite la commande de courses Carrefour",
  "icons": {
    "48": "icons/FourreCar-48.png",
    "96": "icons/FourreCar-96.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.carrefour.fr/s?*"
  ],
  "background": {
    "scripts": ["fourrecar.js"]
  },
  "browser_action": {
    "default_icon": "icons/FourreCar-32.png",
    "default_title": "FourreCar"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "fourrecar.kek@gmail.com",
      "strict_min_version": "42.0"
    }
  }
}